QWidget{
    background-color: white;
    border-bottom: 1px solid rgb(218, 218, 220);
}

QLabel{
    color: rgb(43, 44, 51);
    font: 27px 'Microsoft YaHei';
    font-weight: bold;
    background-color: transparent;
    border: 0px;
}

QLabel:hover{
    color: rgb(86, 86, 91);
}

