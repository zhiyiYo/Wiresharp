QPushButton{
    font: 15px 'Microsoft YaHei';
    font-weight: bold;
    color: rgb(138, 141, 145);
    background-color: transparent;
}

QPushButton:hover{
    color: rgb(161, 163, 167);
}

QPushButton:pressed{
    color: rgb(231, 232, 233);
}